{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "fonts/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/fonts/(.*)", "dest": "/fonts/$1" },
    { "src": "/cyclica-redesigned.html", "dest": "/cyclica-redesigned.html" },
    { "src": "/chat.html", "dest": "/chat.html" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
